      "source": [
        "n = int(input(\"Enter a number: \"))\n",
        "b = 0\n",
        "if n > 1:\n",
        "    for i in range(1,n+1):\n",
        "        a = i*i\n",
        "        #print(i)\n",
        "        #print(a)\n",
        "        b = b + a\n",
        "        #print (b)\n",
        "    print(\"Sum of squares of first\", n,\"natural number is \", b)\n"
